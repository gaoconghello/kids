generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model account {
  id              Int       @id @default(autoincrement())
  username        String    @db.VarChar(50)
  password        String    @db.VarChar(100)
  name            String    @db.VarChar(50)
  age             Int?
  parent_id       Int?
  grade           Int?
  role            String    @db.VarChar(20)
  family_id       Int?
  integral        Int?
  gender          String?   @db.VarChar(1)
  mobile          String?   @db.VarChar(20)
  created_at      DateTime? @db.Timestamp(6)
  updated_at      DateTime? @db.Timestamp(6)
  created_user_id Int?
  updated_user_id Int?
}

model family {
  id              Int       @id @default(autoincrement())
  name            String?   @db.VarChar(50)
  integral        Int?
  is_deadline     String?   @db.VarChar(1)
  deadline        DateTime? @db.Timestamp(6)
  created_at      DateTime? @db.Timestamp(6)
  updated_at      DateTime? @db.Timestamp(6)
  created_user_id Int?
  updated_user_id Int?
}

model homework {
  id                      Int       @id @default(autoincrement())
  name                    String    @db.VarChar(100)
  subject_id              Int?
  subject                 subject?  @relation(fields: [subject_id], references: [id])
  estimated_duration      Int?
  deadline                DateTime? @db.Timestamp(6)
  integral                Int?
  incorrect               Int?
  homework_date           DateTime? @db.Date
  create_review           String?   @db.VarChar(1)
  complete_review         String?   @db.VarChar(1)
  create_review_time      DateTime? @db.Timestamp(6)
  complete_review_time    DateTime? @db.Timestamp(6)
  create_review_user_id   Int?
  complete_review_user_id Int?
  complete_time           DateTime? @db.Timestamp(6)
  is_complete             String?   @db.VarChar(1)
  child_id                Int?
  pomodoro                Int?
  created_at              DateTime? @db.Timestamp(6)
  updated_at              DateTime? @db.Timestamp(6)
  created_user_id         Int?
  updated_user_id         Int?
}

model integral_history {
  id            Int       @id @default(autoincrement())
  integral_id   Int?
  integral_type String?   @db.VarChar(2)
  integral      Int?
  child_id      Int?
  family_id     Int?
  integral_date DateTime? @db.Timestamp(6)
}

model reward {
  id              Int       @id @default(autoincrement())
  name            String?   @db.VarChar(100)
  integral        Int?
  family_id       Int?
  pic             String?   @db.VarChar(36)
  pic_ext         String?   @db.VarChar(20)
  created_at      DateTime? @db.Timestamp(6)
  updated_at      DateTime? @db.Timestamp(6)
  created_user_id Int?
  updated_user_id Int?
}

model reward_history {
  id              Int       @id @default(autoincrement())
  reward_id       Int?
  review          String?   @db.VarChar(1)
  review_time     DateTime? @db.Timestamp(6)
  child_id        Int?
  created_at      DateTime? @db.Timestamp(6)
  updated_at      DateTime? @db.Timestamp(6)
  created_user_id Int?
  updated_user_id Int?
}

model task {
  id                      Int       @id @default(autoincrement())
  name                    String?   @db.VarChar(100)
  integral                Int?
  child_id                Int?
  task_date               DateTime? @db.Date
  create_review           String?   @db.VarChar(1)
  complete_review         String?   @db.VarChar(1)
  create_review_time      DateTime? @db.Timestamp(6)
  complete_review_time    DateTime? @db.Timestamp(6)
  create_review_user_id   Int?
  complete_review_user_id Int?
  complete_time           DateTime? @db.Timestamp(6)
  is_complete             String?   @db.VarChar(1)
  created_at              DateTime? @db.Timestamp(6)
  updated_at              DateTime? @db.Timestamp(6)
  created_user_id         Int?
  updated_user_id         Int?
}

model subject {
  id              Int       @id @default(autoincrement())
  name            String    @db.VarChar(50)
  homeworks       homework[]
  created_at      DateTime? @db.Timestamp(6)
  updated_at      DateTime? @db.Timestamp(6)
  created_user_id Int?
  updated_user_id Int?
}
